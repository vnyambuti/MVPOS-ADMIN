let CategoryPage = class CategoryPage {
    // regex = /^(?:254|\+254|0)?([0-7](?:(?:[129][0-9])|(?:0[0-8])|(4[0-1]))[0-9]{6})$/;
    constructor(route, sho, mclCtrl, navCtrl, httpservice, fb) {
        this.route = route;
        this.sho = sho;
        this.mclCtrl = mclCtrl;
        this.navCtrl = navCtrl;
        this.httpservice = httpservice;
        this.fb = fb;
        this.loading = false;
        this.tabID = 2;
        this.slideCardOpts = {
            slidesPerView: 1.2
        };
    }
    ngOnInit() {
        this.shop = JSON.parse(localStorage.getItem('shop')).id;
        this.categoryForm = this.fb.group({
            shop_id: [this.shop],
            name: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required,
                ]],
        });
    }
    sendCategory() {
        this.loading = true;
        this.httpservice.postData(src_environments_environment__WEBPACK_IMPORTED_MODULE_6__["environment"].url + 'categories', this.categoryForm.value).subscribe((category) => {
            if (!category.success) {
                // this.httpservice.presentSuccessToast(category.message,'bottom').then(()=>{
                // });
                this.httpservice.presentToast(JSON.stringify(category.error));
                this.loading = false;
            }
            else {
                this.loading = false;
                this.sho.ngOnInit();
            }
        });
    }
};
